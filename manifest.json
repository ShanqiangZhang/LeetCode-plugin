{
    "manifest_version": 2,
    "name": "LeetCode Page Modifier",
    "version": "1.0",
    "permissions": ["activeTab", "declarativeContent", "storage"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "page_action": {
      "default_popup": "popup.html"
    }
  }
  